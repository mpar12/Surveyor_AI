{
    "colors": {
      "background": "#ffffff",
      "backgroundMuted": "#f5f5f7",
      "surface": "#ffffff",
      "surfaceSubtle": "#f8f8fa",
      "border": "#e0e0e6",
      "borderStrong": "#c2c2cc",
      "text": "#000000",
      "textMuted": "#4a4a55",
      "textSoft": "#7a7a85",
      "accent": "#000c4c",
      "accentSoft": "rgba(0,12,76,0.08)",
      "accentMuted": "rgba(0,12,76,0.16)",
      "accentOnAccent": "#ffffff",
      "danger": "#b3261e",
      "dangerSoft": "rgba(179,38,30,0.12)",
      "success": "#146c2e",
      "successSoft": "rgba(20,108,46,0.12)",
      "shadowSoft": "rgba(0,0,0,0.08)",
      "shadowStrong": "rgba(0,0,0,0.16)"
    },
    "typography": {
      "fontFamilyBase": "-apple-system,BlinkMacSystemFont,system-ui,Segoe UI,Roboto,sans-serif",
      "fontFamilyMono": "SFMono-Regular,Menlo,Monaco,Consolas,monospace",
      "fontSizes": {
        "xs": 11,
        "sm": 13,
        "md": 15,
        "lg": 17,
        "xl": 20,
        "2xl": 24,
        "3xl": 30
      },
      "lineHeights": {
        "tight": 1.1,
        "snug": 1.25,
        "normal": 1.45,
        "relaxed": 1.6
      },
      "fontWeights": {
        "regular": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "heading": {
        "display": {
          "fontSize": "3xl",
          "fontWeight": "semibold",
          "lineHeight": "snug",
          "letterSpacing": -0.02
        },
        "h1": {
          "fontSize": "2xl",
          "fontWeight": "semibold",
          "lineHeight": "snug",
          "letterSpacing": -0.01
        },
        "h2": {
          "fontSize": "xl",
          "fontWeight": "semibold",
          "lineHeight": "snug"
        },
        "h3": {
          "fontSize": "lg",
          "fontWeight": "medium",
          "lineHeight": "normal"
        }
      },
      "body": {
        "default": {
          "fontSize": "md",
          "fontWeight": "regular",
          "lineHeight": "relaxed"
        },
        "secondary": {
          "fontSize": "sm",
          "fontWeight": "regular",
          "lineHeight": "normal"
        },
        "caption": {
          "fontSize": "xs",
          "fontWeight": "medium",
          "lineHeight": "normal",
          "letterSpacing": 0.02
        }
      },
      "inputText": {
        "fontSize": "md",
        "fontWeight": "regular",
        "lineHeight": "normal"
      },
      "buttonText": {
        "fontSize": "sm",
        "fontWeight": "medium",
        "lineHeight": "normal",
        "letterSpacing": 0.02,
        "textTransform": "none"
      }
    },
    "spacing": {
      "0": 0,
      "1": 2,
      "2": 4,
      "3": 6,
      "4": 8,
      "5": 10,
      "6": 12,
      "7": 14,
      "8": 16,
      "9": 20,
      "10": 24,
      "12": 32,
      "16": 40,
      "20": 48,
      "24": 56,
      "32": 72,
      "40": 96
    },
    "radii": {
      "none": 0,
      "xs": 2,
      "sm": 4,
      "md": 8,
      "lg": 12,
      "xl": 16,
      "2xl": 24,
      "pill": 999
    },
    "components": {
      "appShell": {
        "background": "background",
        "maxWidth": 1200,
        "paddingX": 32,
        "paddingY": 40
      },
      "card": {
        "background": "surface",
        "borderColor": "border",
        "borderWidth": 1,
        "radius": "xl",
        "paddingX": 24,
        "paddingY": 20,
        "shadow": "shadowSoft"
      },
      "primaryButton": {
        "textStyle": "buttonText",
        "background": "accent",
        "backgroundHover": "#001463",
        "backgroundActive": "#00093a",
        "textColor": "accentOnAccent",
        "radius": "md",
        "paddingX": 16,
        "paddingY": 8,
        "gap": 8
      },
      "secondaryButton": {
        "textStyle": "buttonText",
        "background": "surface",
        "backgroundHover": "backgroundMuted",
        "borderColor": "border",
        "borderWidth": 1,
        "textColor": "text",
        "radius": "md",
        "paddingX": 14,
        "paddingY": 8,
        "gap": 8
      },
      "input": {
        "background": "surfaceSubtle",
        "borderColor": "border",
        "borderWidth": 1,
        "borderRadius": "lg",
        "paddingX": 16,
        "paddingY": 12,
        "textStyle": "inputText",
        "placeholderColor": "textSoft"
      },
      "tag": {
        "background": "accentSoft",
        "textColor": "accent",
        "radius": "pill",
        "paddingX": 10,
        "paddingY": 4,
        "textStyle": "body.secondary"
      },
      "chip": {
        "background": "backgroundMuted",
        "textColor": "textMuted",
        "radius": "pill",
        "paddingX": 10,
        "paddingY": 4,
        "gap": 6
      }
    }
  }
  